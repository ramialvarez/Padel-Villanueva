---
import Sidebar from "@/components/admin/Sidebar.astro";
import LayoutComun from "./LayoutComun.astro";

const { title } = Astro.props;
---

<LayoutComun title={title}>
  <div class="flex min-h-screen">
    <Sidebar />
    <div
      class="flex flex-col w-full justify-start m-6 overflow-x-auto lg:overflow-y-hidden xl:overflow-x-hidden overflow-y-auto"
    >
      <slot />
    </div>
  </div>
</LayoutComun>
